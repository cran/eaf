\setkeys{Gin}{width=\textwidth} 
\begin{figure}[t]
\begin{minipage}[b]{0.5\linewidth}
<<eaf, fig=TRUE, echo=FALSE, width=6, height=6>>=
library(eaf)
A1<-read.data.sets(file.path(system.file(package="eaf"),"extdata","ALG_1_dat"))
A2<-read.data.sets(file.path(system.file(package="eaf"),"extdata","ALG_2_dat"))
eafplot(A1,A2,percentiles=c(50))
@
\end{minipage}
\begin{minipage}[b]{0.5\linewidth}
<<eafdiff, fig=TRUE, width=6, height=6>>=
plot.eafdiff(A1,A2)
@
\end{minipage}
\caption{Our second example}
\end{figure}

