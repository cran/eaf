\setkeys{Gin}{width=\textwidth} 
\begin{figure}[t]
<<example, fig=TRUE, echo=FALSE, width=6, height=6>>=
library(eaf)
data(PROVA)
eafplot(time+col~run|inst,groups=alg,data=PROVA)
@
